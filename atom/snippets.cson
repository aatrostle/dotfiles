# Aaron's Snippets!

# This file uses CoffeeScript Object Notation (CSON).
# If you are unfamiliar with CSON, you can read more about it here:
# https://github.com/bevry/cson#what-is-cson

'.source.js':
  'mocha/describe':
    'prefix': 'desc'
    'body': """
      describe('${1:feature}', () => {$2});$3
    """
  'mocha/it':
    'prefix': 'it'
    'body': """
      it('${1:action}', () => {$2});$3
    """
  'Webpack config':
    'prefix': 'wpc'
    'body': """
      module.exports = {
        entry: '',
        output: {
          path: '',
          filename: ''
        },
        devServer: {
          inline: true,
          contentBase: '',
          port: 3333
        },
        module: {
          loaders: []
        }
      }
    """
